[run]
omit =
    */migrations/*
    */management/commands/*
    */templatetags/*
    scripts/*
    config/asgi.py
    config/wsgi.py
    */__init__.py

[report]
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
    # Don't complain about code that should only run when invoked directly
    if __name__ == .__main__:

[settings]
# Measure only application packages
source = apps
